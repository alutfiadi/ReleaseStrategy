<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="AL.ReleaseStrategy.controller.Editstrategy" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
    <Page showHeader="true" title="Edit Release Strategy" showFooter="true" showNavButton="false">
        <content>
        	<BusyDialog id="BusyDialog" />
            <form:Form width="100%" editable="true" visible="true">
                <form:title>
                    <core:Title text="Header" level="Auto"/>
                </form:title>
                <form:formContainers>
                    <form:FormContainer visible="true">
                        <form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Release Strategy" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="iRscode" value="{selectedReleaseStrategy>/Rscode}" placeholder="Release Strategy Code" type="Text" showValueHelp="false" editable="false" visible="true"
										width="auto" valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="8"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label  text="Release Strategy Desc" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"
										visible="true"/>
								</form:label>
								<form:fields>
									<Input id="iRstxt" value="{selectedReleaseStrategy>/Rstxt}" placeholder="Release Strategy " type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" required="true" valueStateText="Invalid entry" maxLength="20"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="false" id="elRgroup">
								<form:label>
									<Label text="Release Group" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="cbGroup" placeholder="Release Group" type="Text" enabled="false" visible="true" width="auto" required="false"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Release Code" design="Standard" width="100%" required="true" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<ComboBox id="cbCode" showSecondaryValues="true"
										items="{ path: 'ReleaseStrategy>/ReleaseCodeSet', sorter: { path: 'Rccode' }}" 
										selectionChange="onChange" selectedKey="{selectedReleaseStrategy>/Rccode}">
										<core:ListItem key="{ReleaseStrategy>Rccode}" text="{ReleaseStrategy>Rccode} - {ReleaseStrategy>Rctxt}"
											additionalText="{ReleaseStrategy>Rgcode}"/>
									</ComboBox>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Time Limit (days)" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<Input id="iLimit" value="{selectedReleaseStrategy>/Limit}"  placeholder="Time limit days" type="Number" showValueHelp="false" enabled="true" visible="true" width="25%" valueHelpOnly="false"
										required="false" valueStateText="Invalid entry"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="Requester" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
								</form:label>
								<form:fields>
									<MultiComboBox id="cbRequester" placeholder="Choose Max 3 User Requester" editable="true" enabled="true"
										visible="true" width="auto" valueState="None" selectionFinish="validasiUser"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
                        
                        <form:title/>
                    </form:FormContainer>
                </form:formContainers>
                <form:layout>
                    <form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
                </form:layout>
            </form:Form>
