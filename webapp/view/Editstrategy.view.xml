										placeholder="Level To" selectionChange="checkLevelFrom" items="{ path: 'MasterData>/MasterDataGolonganSet' }"  selectedKey="{selectedReleaseStrategy>/Perskto}">
										<core:ListItem key="{MasterData>Persg}" text="{MasterData>Persg}" />
